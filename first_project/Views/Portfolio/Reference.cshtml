@{
    ViewBag.Title = "Reference";
}

<link rel="stylesheet" href="/Content/Style.css" />

<div class="ref">
    <h1>References</h1>

    <ul>
        
        @for (int i=0; i < Model.Name.Length; i++)
            {
                <li><strong>@Model.Name[i]</strong></li>
                @Model.Status[i]
            }

    </ul>
    <a href="/Portfolio/Index">Back</a>
</div> 